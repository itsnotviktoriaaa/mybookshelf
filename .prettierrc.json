{
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "trailingComma": "es5",
  "bracketSameLine": true,
  "printWidth": 100,
  "endOfLine": "auto",
  "sortingMethod": "alphabetical",
  "plugins": ["prettier-plugin-sort-imports"]
}
